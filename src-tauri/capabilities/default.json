{
  "identifier": "default",
  "description": "Default capabilities for Shoulders",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "deep-link:default",
    "shell:allow-open",
    "updater:default",
    "process:allow-restart"
  ]
}
